<Application
    x:Class="OpenSilverAttachDetach.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:OpenSilverAttachDetach">
    <Application.Resources>
        <ControlTemplate x:Key="ExampleTemplate" TargetType="local:TemplatedButton">
            <ContentPresenter/>
        </ControlTemplate>

        <Style TargetType="local:TemplatedButton">
            <Setter Property="Template" Value="{StaticResource ExampleTemplate}"/>
        </Style>
    </Application.Resources>
</Application>
